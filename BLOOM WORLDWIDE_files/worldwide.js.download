var x = 0;
var link = 'http://www.bloomworldwide.net/worldwide/'; /* The text */
var speed1 = 50; /* The speed/duration of the effect in milliseconds */

function worldwideLink() {
  if (x < link.length) {
    document.getElementById("wwLink").innerHTML += link.charAt(x);
    x++;
    setTimeout(worldwideLink, speed1);
  }
}
